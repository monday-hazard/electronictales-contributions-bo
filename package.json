    "type": "git",
    "url": "git+https://github.com/monday-hazard/electronictales-contributions-bo.git"
  },
  "keywords": [
    "electronic",
    "tales",
    "contributing",
    "developers",
    "juniors",
    "back-office"
  ],
  "author": "The Fabulous Four",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monday-hazard/electronictales-contributions-bo/issues"
  },
  "homepage": "https://github.com/monday-hazard/electronictales-contributions-bo#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.9",
    "nodemailer": "^6.6.5",
    "nodemailer-express-handlebars": "^4.0.0",
    "react-quill": "^1.3.5",
    "request": "^2.88.2"
  },
  "devDependencies": {
